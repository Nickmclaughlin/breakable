<ul class="admin-list">
  <% @photos.each do |photo| %>
    <li><%= image_tag photo.photo.url, style: "width: 400px; height: 400px;"%>
      <%= link_to "Delete", admin_user_photo_path(photo.user, photo), class: "button tiny alert", method: :delete, data: {confirm: 'Are you sure you want to delete photo?'} %>
    </li>
  <% end %>
</ul>
<%= paginate @photos %>
